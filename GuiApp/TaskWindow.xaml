<Window x:Class="Qfe.TaskWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Qfe"
        mc:Ignorable="d"
        Title="TaskWindow" Height="450" Width="800">
    <DockPanel>
        <StackPanel Orientation="Vertical">
            <StackPanel Orientation="Horizontal">
                <Label>Czas wykonania:</Label>
                <Label x:Name="exeutionTimeLabel"/>
            </StackPanel>
            <Label HorizontalAlignment="Center">Wyniki</Label>
            <TextBlock x:Name="algorithmResults" Width="300" Margin="5"/>
        </StackPanel>
        <StackPanel>
            <Label HorizontalAlignment="Center">TODO: wykres</Label>
        </StackPanel>
    </DockPanel>
</Window>
